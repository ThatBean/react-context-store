{
  "name": "react-context-store",
  "version": "0.2.0-dev.0",
  "author": "thatbean",
  "license": "MIT",
  "description": "Make use of React context with: ContextProvider, ContextConnector and ContextStore",
  "keywords": [
    "connector",
    "provider",
    "store",
    "context",
    "react"
  ],
  "repository": "github:thatbean/react-context-store",
  "main": "library/index.js",
  "scripts": {
    "// script ======================": "",
    "script-base": "babel-node ./script quiet",
    "script-pack": "babel-node ./script quiet pack",
    "script-publish": "babel-node ./script pack publish",
    "script-publish-dev": "babel-node ./script pack publish-dev",
    "script-generate-spec": "babel-node ./script/generateSpec",
    "// build =======================": "",
    "build-library": "babel ./source --out-dir ./output-gitignore/library",
    "build-library-dev": "cross-env BABEL_ENV=dev npm run build-library -- --watch",
    "// =============================": "",
    "prepack": "echo \"Error: pack with script-*\" && exit 1"
  },
  "peerDependencies": {
    "react": "^16",
    "prop-types": "^15"
  },
  "devDependencies": {
    "dr-dev": "0.0.6-dev.3",
    "dr-dev-babel-react": "0.0.6-dev.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "prop-types": "^15.7.2"
  }
}
